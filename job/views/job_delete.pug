extends ../../views/layout

block content
  h1= 'Name: ' + job.name
  p= 'Description: ' + job.description

  p Do you really want to delete this job?

  form(method='POST' action='')
    div.form-group
      input#jobid.form-control(type='hidden',name='jobid', required='true', value=job._id )

    button.btn.btn-primary(type='submit') Delete
